      <h2 class="font-weight-bold">User Login</h2>
      <span>Login for the exesting user</span>
      <!-- <% if (locals.Logout) {
         %>
        <div class="alert alert-success text-center">Logout successful</div>
      <% 
    } %> -->
      
    </div>

    
    <form action="/login" method="POST">
      <div class="form-group">
        <input
          type="email"
          class="form-control"
          placeholder="Enter Email"
          name="email"
        />
      </div>
      <div class="form-group">
        <input
          type="Password"
          class="form-control"
          name="password"
          placeholder="Enter Password"
        />
